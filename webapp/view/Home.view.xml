						<Title id="productTile" text="{i18n>ContentConsultation}" level="H3" titleStyle="H2" class="sapUiTinyMarginTopBottom"/>
						<Text id="_IDGenText2" text="{i18n>DetailTitle}" class="sapUiTinyMarginBottom"/>
						<Link id="_IDGenLink3" text="{i18n>accessLink}"  press="consultation"/>
					</l:VerticalLayout>
				</l:BlockLayoutCell>
				<l:BlockLayoutCell id="_IDGenBlockLayoutCell4" backgroundColorSet="ColorSet11" backgroundColorShade="ShadeD">
					<l:VerticalLayout id="_IDGenVerticalLayout3">
						<core:Icon id="_IDGenIcon3" src="sap-icon://customer-financial-fact-sheet" size="2.5rem" color="Default"/>
						<Title id="_IDGenTitle1" text="{i18n>ContentModifications}" level="H3" titleStyle="H2" class="sapUiTinyMarginTopBottom"/>
						<Text id="_IDGenText3" text="{i18n>DetailTitleModif}" class="sapUiTinyMarginBottom"/>
						<Link id="_IDGenLink5" text="{i18n>accessLink}" press="modification" />
					</l:VerticalLayout>
				</l:BlockLayoutCell>
				
				
			</l:BlockLayoutRow>
			<l:BlockLayoutRow id="_IDGenBlockLayoutRow3">

			<l:BlockLayoutCell id="_IDGenBlockLayoutCell6" backgroundColorSet="ColorSet11" backgroundColorShade="ShadeB">
