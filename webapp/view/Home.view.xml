				</l:BlockLayoutCell>
			</l:BlockLayoutRow>
			<l:BlockLayoutRow id="_IDGenBlockLayoutRow2">
				<l:BlockLayoutCell id="_IDGenBlockLayoutCell2" backgroundColorSet="ColorSet11" backgroundColorShade="ShadeD">
					<l:VerticalLayout id="_IDGenVerticalLayout1">
						<core:Icon id="_IDGenIcon1" src="sap-icon://activity-2" size="2.5rem" color="Default"/>
						<Title id="administrationTile" text="{i18n>administrationTitle}" level="H3" titleStyle="H2" class="sapUiTinyMarginTopBottom"/>
						<Text id="_IDGenText1" text="{i18n>administrationContent}" class="sapUiTinyMarginBottom"/>
						<Link id="_IDGenLink1" text="{i18n>accessLink}" press="miseaurebut" />
					</l:VerticalLayout>
				</l:BlockLayoutCell>
				<l:BlockLayoutCell id="_IDGenBlockLayoutCell3" backgroundColorSet="ColorSet11" backgroundColorShade="ShadeB">
