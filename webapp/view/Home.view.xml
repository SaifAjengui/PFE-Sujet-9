						<Text id="_IDGenText6" text="{i18n>DetailTitleVente}" class="sapUiTinyMarginBottom"/>
						<Link id="_IDGenLink7" text="{i18n>accessLink}"  press="vente"/>
					</l:VerticalLayout>
			</l:BlockLayoutCell>	
			<l:BlockLayoutCell id="_IDGenBlockLayoutCell5" backgroundColorSet="ColorSet11" backgroundColorShade="ShadeD">
					<l:VerticalLayout id="_IDGenVerticalLayout4">
						<core:Icon id="_IDGenIcon4" src="sap-icon://action" size="2.5rem" color="Default"/>
						<Title id="_IDGenTitle2" text="{i18n>ContentTransfert}" level="H3" titleStyle="H2" class="sapUiTinyMarginTopBottom"/>
						<Text id="_IDGenText4" text="{i18n>DetailTitleTransfert}" class="sapUiTinyMarginBottom"/>
						<Link id="_IDGenLink6" text="{i18n>accessLink}" press="transfert" />
					</l:VerticalLayout>
			</l:BlockLayoutCell>
			
			
